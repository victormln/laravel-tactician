{
    "name": "victormln/laravel-tactician",
    "description": "Laravel implementation of the Tactician Command Bus",
    "keywords": [
      "laravel",
      "laravel-tactician",
      "Command Bus",
      "Tactician"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Víctor Molina Ferreira",
            "email": "victor.molinaf@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "league/tactician": "1.0.*"
    },
    "require-dev": {
        "phpunit/phpunit" : "7.*",
        "scrutinizer/ocular": "~1.1",
        "mockery/mockery" : "0.9.*",
        "orchestra/testbench": "~3.5"
    },
    "autoload": {
        "psr-4": {
            "Victormln\\LaravelTactician\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Victormln\\LaravelTactician\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
      "laravel": {
        "providers": [
          "Victormln\\LaravelTactician\\Providers\\LaravelTacticianServiceProvider"
        ]
      }
    }
}
